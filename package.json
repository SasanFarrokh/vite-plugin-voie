    "glob": "^7.1.6",
    "pify": "^5.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@ls-lint/ls-lint": "^1.9.2",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.25",
    "@types/pify": "^3.0.2",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "tsup": "^3.1.2",
    "typescript": "^3.9.7",
    "vite": "^1.0.0-rc.3",
    "yorkie": "^2.0.0"
